<h1>Edit <%= @item.name %></h1>

<%= simple_form_for(@item) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <%= f.submit 'Update item', class: 'btn btn-primary' %>
<% end %>
<%= link_to 'Cancel', items_path %>
